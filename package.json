  },
  "files": [
    "LICENSE",
    "README.md",
    "config/*.sample",
    ".eslintrc.js",
    ".gitignore",
    ".vscode",
    "index.js",
    "src",
    "package.json",
    "yarn.lock"
  ],
  "main": "main.js",
  "scripts": {
    "dev": "NODE_ENV=development node ./index.js",
    "start": "node ./index.js",
    "prd": "if [[ -d .git ]] && [[ 1 == '1' ]]; then git pull; fi; if [[ ! -z ${NODE_PACKAGES} ]]; then /usr/local/bin/npm install ${NODE_PACKAGES}; fi; if [[ ! -z ${UNNODE_PACKAGES} ]]; then /usr/local/bin/npm uninstall ${UNNODE_PACKAGES}; fi; if [ -f /home/container/package.json ]; then /usr/local/bin/npm install; fi; /usr/local/bin/node /home/container/index.js",
    "lint": "eslint src",
    "build": "node ./build.js"
  },
  "engines": {
    "node": "^16.13",
    "npm": "^8.2"
  },
  "dependencies": {
    "config": "^3.3.6",
    "discord.js": "^13.3.1",
    "forever": "^4.0.1",
    "forever-monitor": "^3.0.3",
    "lodash.merge": "^4.6.2",
    "log4js": "^6.3.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
